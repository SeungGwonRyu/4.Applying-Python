    "from random import randint\n",
    "\n",
    "\n",
    "def generate_numbers(n):\n",
    "\n",
    "    numbers = []\n",
    "\n",
    "    while len(numbers) < n:\n",
    "        num = randint(1, 45)\n",
    "        if num not in numbers:\n",
    "            numbers.append(num)\n",
    "\n",
    "    return numbers\n",
    "\n",
    "\n",
    "# 테스트 코드\n",
    "print(generate_numbers(6))\n"
   ]
